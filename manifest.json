{
    "manifest_version": 2,
    "name": "Auto Form Filler",
    "version": "1.0",
    "description": "Automatically fills forms on web pages.",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
      }
    ]
  }
  